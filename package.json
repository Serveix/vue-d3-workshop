    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "register-service-worker": "^1.5.2",
    "topojson-client": "^3.0.0",
    "versor": "0.0.4",
    "vue": "^2.5.21",
    "vue-axios": "^2.1.4",
    "vue-class-component": "^6.0.0",
    "vue-markdown": "^2.2.4",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuebar": "0.0.20",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/chroma-js": "^1.4.1",
    "@types/crossfilter": "0.0.32",
